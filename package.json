    "deploy": "aws s3 cp build/ s3://walkerangell.com/ --recursive --cache-control \"max-age=94608000\" --expires \"Thu, 31 Dec 2099 20:00:00 GMT\" --exclude \"*.html\" && aws s3 cp build/ s3://walkerangell.com/ --recursive --exclude \"*\" --include \"*.html\"",
